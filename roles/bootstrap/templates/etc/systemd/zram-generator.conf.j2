## {{ ansible_managed }}
# See /usr/share/doc/systemd-zram-generator/zram-generator.conf.example
# and/or zram-generator.conf(5) for a list of available options.
[{{ zram_device }}]
zram-size = min(ram / 2, {{ zram_minimal_size }})
compression-algorithm = {{ zram_compression }}
fs-type = {{ zram_filesystem }}
swap-priority = {{ zram_priority }}